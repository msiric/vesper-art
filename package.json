{
  "name": "marketplace",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "yarn run transpile && cross-env NODE_ENV=testing jest --silent --runInBand ./dist",
    "test:fresh": "yarn run seed:test-db && yarn run test",
    "test:fresh-entities": "yarn run seed:test-db-entities && yarn run test",
    "test:fresh-s3": "yarn run seed:test-db-s3 && yarn run test",
    "test:coverage": "yarn run test -- --coverage",
    "test:watch": "yarn run transpile:watch-silent \"tsc-alias && cross-env NODE_ENV=testing jest --watch --silent --runInBand ./dist\"",
    "client": "cd client && yarn start",
    "server": "cross-env NODE_ENV=development node ./dist/bin/www",
    "server:dev": "cross-env NODE_ENV=development nodemon ./dist/bin/www",
    "dev": "npx kill-port 3000 && npx kill-port 5000 && concurrently --kill-others \"yarn run transpile:watch\" \"yarn run server:dev\" \"yarn run client\" \"yarn run webhook\"",
    "prod": "cd client && yarn install && yarn build:deploy && cd .. && yarn run transpile && cross-env NODE_ENV=production node ./dist/bin/www",
    "staging": "cd client && yarn install && yarn build:deploy && cd .. && yarn run transpile && cross-env NODE_ENV=staging node ./dist/bin/www",
    "webhook": "stripe listen --forward-to localhost:5000/webhook/events",
    "alias": "tsc-alias",
    "transpile": "tsc && yarn run alias",
    "transpile:watch": "yarn run transpile -- -w",
    "transpile:watch-silent": "tsc-watch --onSuccess",
    "transpile:copy-files": "tsc && yarn run copy-files && yarn run alias",
    "seed:dev-db": "cross-env NODE_ENV=development node ./dist/fixtures/index",
    "seed:test-db": "cross-env NODE_ENV=seeding node ./dist/tests/fixtures/index",
    "seed:test-db-entities": "cross-env NODE_ENV=seeding node ./dist/tests/fixtures/index skipS3",
    "seed:test-db-s3": "cross-env NODE_ENV=seeding node ./dist/tests/fixtures/index skipEntities",
    "copy-files": "copyfiles ./common/assets/* ./dist",
    "postinstall": "yarn run transpile:copy-files",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "proxy:dev": "flyctl proxy 15432:5432 -a vesper-art-development",
    "proxy:test": "flyctl proxy 25432:5432 -a vesper-art-testing"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.27.1",
    "async": "^3.1.0",
    "aws-sdk": "^2.486.0",
    "axios": "^0.19.2",
    "base64-stream": "^1.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^7.0.0",
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.4.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "currency.js": "^1.2.2",
    "date-fns": "^2.28.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^6.2.1",
    "express-session": "^1.16.2",
    "form-data": "^3.0.0",
    "helmet": "^4.2.0",
    "image-size": "^0.8.3",
    "jsonwebtoken": "^8.5.1",
    "mock-aws-s3": "^4.0.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^6.2.1",
    "nodemon": "^1.19.1",
    "number-abbreviate": "^2.0.0",
    "pg": "^8.5.1",
    "query-string": "^6.11.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.1",
    "socket.io": "^2.2.0",
    "stripe": "^7.4.0",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.6.11",
    "tsc-watch": "^4.5.0",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "dotenv": "^8.1.0",
    "eslint": "^8.18.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^24.9.0",
    "js-yaml": "^4.1.0",
    "kill-port": "^1.6.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  }
}
